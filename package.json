{
  "name": "xmws",
  "version": "0.0.1",
  "description": "`xMws` is an Extra Web UI tools based on Amazon MWS (Marketplace Web Service) for sellers. Seriously!",
  "repository": "git@bitbucket.org:xhubert/xmws.git",
  "author": "Hubert Fan",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=8.12.0"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development backpack dev",
    "build": "cross-env NODE_ENV=production nuxt build && backpack build",
    "serve": "cross-env NODE_ENV=production node build/main.js",
    "start": "npm run build && npm run serve",
    "heroku-postbuild": "npm run build",
    "pm2": "cross-env NODE_ENV=production pm2 start build/main.js --name xmws",
    "precommit": "npm run lint",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore ."
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.4.1",
    "amazon-mws": "^0.0.23",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-logger": "^3.2.0",
    "koa-passport": "^4.1.1",
    "koa-router": "^7.4.0",
    "koa-session": "^5.10.1",
    "lodash.drop": "^4.1.1",
    "lodash.filter": "^4.6.0",
    "lodash.find": "^4.6.0",
    "lodash.indexof": "^4.0.5",
    "lodash.map": "^4.6.0",
    "lodash.omit": "^4.5.0",
    "lodash.orderby": "^4.6.0",
    "moment": "^2.24.0",
    "mws-api": "^3.0.0",
    "nuxt": "^2.4.5",
    "passport-local": "^1.0.0",
    "vuetify": "^1.5.5",
    "vuex-persistedstate": "^2.5.4",
    "winston": "^3.2.1",
    "xlsx": "^0.14.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "backpack-core": "^0.8.3",
    "cross-env": "^5.2.0",
    "eslint": "^5.15.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-vuetify": "^0.2.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-html": "^5.0.3",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vuetify-loader": "^1.2.1"
  }
}
